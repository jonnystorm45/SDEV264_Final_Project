<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/oip"
    tools:context=".PersonalActivity">


    <TextView
        android:id="@+id/workout_pref"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="119dp"
        android:layout_marginTop="80dp"
        android:text="Personal"
        android:textColor="#F4F2F2"
        android:textSize="34sp"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/goal_2"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="166dp"
        android:text="Goals:"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:background="@color/black"/>

    <TextView
        android:id="@+id/goalsId3"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="166dp"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        android:background="@color/black"/>

    <TextView
        android:id="@+id/training_type"
        android:layout_width="175dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="225dp"
        android:text="Type Of Training:"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:background="@color/black"/>

    <TextView
        android:id="@+id/trainingId2"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="225dp"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        android:background="@color/black"/>

    <TextView
        android:id="@+id/time"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="300dp"
        android:text="Timetable :"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:background="@color/black"/>

    <TextView
        android:id="@+id/time3"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="300dp"
        android:inputType="text"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        android:background="@color/black"/>

    <TextView
        android:id="@+id/goal"
        android:layout_width="120dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="350dp"
        android:text="Goal Weight:"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:background="@color/black"/>

    <TextView
        android:id="@+id/goal3"
        android:layout_width="150dp"
        android:layout_height="25dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="350dp"
        android:ems="10"
        android:inputType="numberSigned"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"
        android:background="@color/black"/>

    <Button
        android:id="@+id/backBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="202dp"
        android:background="#000000"
        android:text="Back"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#000000"
        android:textColorHint="#000000"
        android:textColorLink="#000000"
        app:rippleColor="#000000" />

    <Button
        android:id="@+id/logoutId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="111dp"
        android:onClick="onClick"
        android:text="Logout" />


</RelativeLayout>